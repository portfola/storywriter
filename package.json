    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-config": "^1.5.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.17.4",
