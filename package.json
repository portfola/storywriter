{
  "name": "storywriter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "homepage": "/storywriter",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "tsc --noEmit && expo lint",
    "validate": "npm run type-check && npm run test",
    "build:web": "expo export --platform web"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.848.0",
    "@aws-sdk/client-transcribe-streaming": "^3.848.0",
    "@elevenlabs/client": "^0.3.0",
    "@elevenlabs/elevenlabs-js": "^2.5.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.2",
    "axios": "^1.7.9",
    "base64-js": "^1.5.1",
    "dot-env": "^0.0.1",
    "expo": "~53.0.17",
    "expo-build-properties": "~0.13.1",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.6",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-screen-orientation": "~8.0.2",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-config": "^1.5.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "^13.15.0",
    "together-ai": "^0.17.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@expo/config-plugins": "^9.0.12",
    "@expo/vector-icons": "^14.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/prop-types": "^15.7.14",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "expo-module-scripts": "^4.0.4",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|zustand)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/.expo/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/.expo/**"
    ]
  },
  "private": true
}
