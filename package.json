  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@expo/config-plugins": "^9.0.12",
    "@expo/vector-icons": "^14.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
