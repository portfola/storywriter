    "@react-navigation/native-stack": "^7.3.21",
    "axios": "^1.10.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "base64-js": "^1.5.1",
    "expo": "~53.0.17",
    "expo-build-properties": "~0.13.1",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.6",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-screen-orientation": "~8.0.2",
