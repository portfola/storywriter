{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/../*": [
        "../*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": ["node", "jest", "prop-types"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}